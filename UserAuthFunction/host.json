{
  "version": "2.0",
  "extensions": {
    "queues": {
      "maxDequeueCount": 3 //Default is 5
    }
  },
  "logging": {
    "fileLoggingMode": "always",
    "logLevel": {
      "OrderFunction": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}